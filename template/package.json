{
  "name": "HelloWorld",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "pod-install": "cd ios && pod install && cd ..",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-community/push-notification-ios": "1.10.1",
    "@react-native-firebase/analytics": "15.3.0",
    "@react-native-firebase/app": "15.3.0",
    "@react-native-firebase/crashlytics": "15.3.0",
    "@react-native-firebase/messaging": "15.3.0",
    "@react-native-masked-view/masked-view": "0.2.7",
    "@react-navigation/bottom-tabs": "6.3.3",
    "@react-navigation/material-top-tabs": "6.2.3",
    "@react-navigation/native": "6.0.12",
    "@react-navigation/native-stack": "6.8.0",
    "@react-navigation/stack": "6.2.3",
    "apisauce": "2.1.6",
    "i18next": "21.9.1",
    "moment": "2.29.4",
    "react": "18.0.0",
    "react-i18next": "11.18.4",
    "react-native": "0.69.5",
    "react-native-device-info": "10.0.2",
    "react-native-gesture-handler": "2.6.0",
    "react-native-keychain": "8.1.1",
    "react-native-localize": "2.2.3",
    "react-native-mmkv": "2.4.3",
    "react-native-modal": "13.0.1",
    "react-native-pager-view": "5.4.25",
    "react-native-push-notification": "8.1.1",
    "react-native-reanimated": "2.10.0",
    "react-native-safe-area-context": "4.3.3",
    "react-native-screens": "3.17.0",
    "react-native-tab-view": "3.1.1",
    "react-native-vector-icons": "9.2.0",
    "rn-fetch-blob": "0.12.0",
    "zustand": "4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/runtime": "^7.18.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.5",
    "@types/react-native-push-notification": "^8.1.1",
    "@types/react-native-vector-icons": "^6.4.11",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@typescript-eslint/scope-manager": "^5.30.0",
    "@typescript-eslint/typescript-estree": "^5.30.0",
    "@typescript-eslint/utils": "^5.30.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "singleQuote": false,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "tabWidth": 2,
    "printWidth": 120
  },
  "eslintConfig": {
    "root": true,
    "extends": "@react-native-community",
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "overrides": [
      {
        "files": [
          "*.js",
          "*.jsx",
          "*.ts",
          "*.tsx"
        ],
        "rules": {
          "react-native/no-inline-styles": 0,
          "@typescript-eslint/no-shadow": [
            "error"
          ],
          "no-shadow": "off",
          "no-undef": "off",
          "jsx-quotes": "off",
          "quotes": "off"
        }
      }
    ]
  }
}
